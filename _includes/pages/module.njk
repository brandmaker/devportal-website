<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script>
    // Check browser support
    if (typeof(Storage) !== "undefined") {
      // Store
      if (localStorage.getItem("{{ module }}_used_productVersion")) {      
        window.location.href = window.location.href + localStorage.getItem("{{ module }}_used_productVersion");
      } else {
        window.location.href = window.location.href + "{{ destination }}";
      }
      
    } else {
      window.location.href = window.location.href + "{{ destination }}";
    } 
  </script>
</head>
<body>
  <!-- Redirect page -->
</body>
</html>